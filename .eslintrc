{
  "parser": "babel-eslint",
  "plugins": [
    "react"
  ],
  "globals": {
    "d3": true,
    "module": true,
    "inject": true,
    "require": true,
    "document": true,
    "window": true,
    "fetch": true,
    "VERSION": true,
    "location": true,
    "history": true,
    "FileReader": true,
    "Blob": true
  },
  "env": {
    "es6": true,
    "node": true,
    "mocha": true
  },
  "rules": {
    "indent": ["warn", 2],
    "react/jsx-indent": ["warn", 2],
    "no-unused-vars": 1,
    "react/jsx-uses-react": 1,
    "react/jsx-uses-vars": 1,
    "react/jsx-curly-spacing": [0, "always"],
    "react/jsx-key": 1,
    "quotes": [ 1, "single" ],
    "no-undef": 1,
    "global-strict": 0,
    "no-extra-semi": 1,
    "no-underscore-dangle": 0,
    "no-console": 0,
    "no-trailing-spaces": [1, { "skipBlankLines": true }],
    "no-unreachable": 1,
    "no-alert": 0,
    "object-curly-spacing": [0, "always"]
  },
  "settings": {
  }
}
